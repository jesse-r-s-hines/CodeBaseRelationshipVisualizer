{
    "name": "code-structure-visualization",
    "description": "Visualize relationships between source code files.",
    "version": "0.0.1",
    "publisher": "JesseHines0",
    "private": true,
    "license": "MIT",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Visualization", "Debuggers"
    ],
    "activationEvents": [
        "onCommand:codeStructureVisualization.start",
        "onWebviewPanel:codeStructureVisualization"
    ],
    "contributes": {
        "commands": [
            {
                "command": "codeStructureVisualization.start",
                "title": "Visualize relationships between source code files",
                "category": "Code Structure Visualization"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint . --ext .ts,.tsx",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/node": "^12.12.0",
        "@types/vscode": "^1.47.0",
        "@types/vscode-webview": "^1.57.0",
        "@typescript-eslint/eslint-plugin": "^4.16.0",
        "@typescript-eslint/parser": "^4.16.0",
        "eslint": "^7.21.0",
        "typescript": "^4.4.3"
    }
}
